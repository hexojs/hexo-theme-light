<div class="alignleft">
  <% if (theme.footer != false) { %>
  <%= theme.footer %>
  <% } else if (config.author){ %>
  &copy; <%= new Date().getFullYear() %> <%= config.author %>
  <% } else { %>
  &copy; <%= new Date().getFullYear() %> <%= config.title %>
  <% } %>
</div>
<div class="clearfix"></div>